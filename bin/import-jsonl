#!/usr/bin/env python

import argparse

from offeneregister.jsonl2neo import JsonlImporter

parser = argparse.ArgumentParser(
       description='Import bzip2 jsonl file into neo4j database')

parser.add_argument('filename', help='Filename of bzip2 jsonl file')
# parser.add_argument('url', nargs='+', help='URL of neo4j database')
args = parser.parse_args()
importer = JsonlImporter()
importer.import_offene_register_file(args.filename, False)
exit(0)
